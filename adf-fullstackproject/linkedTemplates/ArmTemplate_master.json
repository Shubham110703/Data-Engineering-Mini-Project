{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-fullstackproject"
		},
		"ABS_fullStackProject_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ABS_fullStackProject'"
		},
		"DB_fullstackproject_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'DB_fullstackproject'"
		},
		"MDA_fullstackproject_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MDA_fullstackproject'"
		},
		"DB_fullstackproject_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlservers01.database.windows.net"
		},
		"DB_fullstackproject_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "DB_fullstackdata"
		},
		"DB_fullstackproject_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "testuser"
		},
		"MDA_fullstackproject_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "test"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-fullstackproject_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ABS_fullStackProject_connectionString": {
						"value": "[parameters('ABS_fullStackProject_connectionString')]"
					},
					"DB_fullstackproject_password": {
						"value": "[parameters('DB_fullstackproject_password')]"
					},
					"MDA_fullstackproject_connectionString": {
						"value": "[parameters('MDA_fullstackproject_connectionString')]"
					},
					"DB_fullstackproject_properties_typeProperties_server": {
						"value": "[parameters('DB_fullstackproject_properties_typeProperties_server')]"
					},
					"DB_fullstackproject_properties_typeProperties_database": {
						"value": "[parameters('DB_fullstackproject_properties_typeProperties_database')]"
					},
					"DB_fullstackproject_properties_typeProperties_userName": {
						"value": "[parameters('DB_fullstackproject_properties_typeProperties_userName')]"
					},
					"MDA_fullstackproject_properties_typeProperties_database": {
						"value": "[parameters('MDA_fullstackproject_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}